{
  "name": "maxmobility",
  "description": "NativeScript Application",
  "license": "SEE LICENSE IN <your-license-filename>",
  "readme": "NativeScript Application",
  "repository": "<fill-your-repository-here>",
  "nativescript": {
    "id": "com.permobil.smarteval",
    "tns-android": {
      "version": "5.2.1"
    }
  },
  "scripts": {
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "nuke": "rm -rf node_modules platforms hooks",
    "lint": "tslint \"app/**/*.ts\"",
    "reset": "rm -rf hooks node_modules platforms && npm run css.clean && npm run js.clean",
    "clean": "npm run js.clean && npm run css",
    "css.clean": "find app -type f -iname '*.css' -and ! -iname 'font-awesome.min.css' -delete",
    "css.generate": "node -e 'require(\"./node_modules/nativescript-dev-sass/lib/converter.js\").convert(console, \".\");'",
    "css": "npm run css.clean && npm run css.generate",
    "js.clean": "npx rimraf -- app/**/*.js",
    "ns-bundle": "ns-bundle",
    "publish-ios-bundle": "npm run ns-bundle --ios --publish-app",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "start-android-bundle": "npm run ns-bundle --android --run-app",
    "start-ios-bundle": "npm run ns-bundle --ios --run-app",
    "build-geolocation": "cd ./plugins/nativescript-geolocation && npm i && tsc && rm -rf *.tgz && npm pack",
    "build-android-bundle": "npm run ns-bundle --android --build-app",
    "build-ios-bundle": "npm run ns-bundle --ios --build-app",
    "start": "tns run ios --emulator --syncAllFiles",
    "make-changelog": "github_changelog_generator -u PushTracker -p EvalApp",
    "bundle-android": "cd apps/eval-mobile && tns run android --bundle --env.aot --env.uglify",
    "bundle-ios": "cd apps/eval-mobile && tns run ios --bundle --env.aot --env.uglify",
    "ios.hmr": "cd apps/eval-mobile && tns run ios --bundle --hmr",
    "ios": "cd apps/eval-mobile && tns run ios --bundle",
    "android.hmr": "cd apps/eval-mobile && tns run android --bundle --hmr",
    "android": "cd apps/eval-mobile && tns run android --bundle",
    "ns-setup": "npm uninstall -g nativescript && npm install -g nativescript",
    "ios-release": "npm run nuki && cd apps/eval-mobile && tns build ios --release",
    "android-release": "node ./apps/eval-mobile/tools/android-release.js",
    "precommit": "lint-staged",
    "profile-android-layouts": "adb shell dumpsys gfxinfo com.permobil.smartevaluation > layout-profile.txt",
    "android-framestats": "adb shell dumpsys gfxinfo com.permobil.smartevaluation framestats > framestats.txt"
  },
  "dependencies": {
    "@_/nativescript-swift-fix": "file:./plugins/swift-fix",
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngx-translate/core": "~11.0.1",
    "buffer": "~5.2.1",
    "email-validator": "~2.0.4",
    "kinvey-nativescript-sdk": "~3.12.4",
    "mustache": "~2.3.0",
    "nativescript-angular": "~7.2.2",
    "nativescript-appversion": "^1.4.1",
    "nativescript-barcodescanner": "~3.0.3",
    "nativescript-bluetooth": "file:../bluetooth",
    "nativescript-camera": "~4.2.0",
    "nativescript-carousel": "~4.1.0",
    "nativescript-drop-down": "~4.0.1",
    "nativescript-email": "1.5.3",
    "nativescript-feedback": "1.3.5",
    "nativescript-floatingactionbutton": "5.0.0",
    "nativescript-geolocation": "file:./plugins/nativescript-geolocation.tgz",
    "nativescript-gif": "~4.0.2",
    "nativescript-imagecropper": "~1.0.4",
    "nativescript-insomnia": "^1.2.2",
    "nativescript-intl": "~3.0.0",
    "nativescript-iqkeyboardmanager": "~1.4.0",
    "nativescript-loading-indicator": "~2.4.0",
    "nativescript-localstorage": "~2.0.0",
    "nativescript-mapbox": "file:./plugins/nativescript-mapbox-4.3.1.tgz",
    "nativescript-master-technology": "^1.1.1",
    "nativescript-modal-datetimepicker": "~1.1.12",
    "nativescript-ngx-fonticon": "~4.2.0",
    "nativescript-orientation": "~2.2.1",
    "nativescript-permissions": "~1.2.3",
    "nativescript-push-notifications": "~1.1.6",
    "nativescript-sentry": "~1.8.0",
    "nativescript-snackbar": "~4.1.0",
    "nativescript-theme-core": "~1.0.4",
    "nativescript-toasty": "~1.2.0",
    "nativescript-ui-listview": "~5.2.0",
    "nativescript-youtubeplayer": "~3.0.1",
    "reflect-metadata": "~0.1.12",
    "rxjs": "~6.4.0",
    "rxjs-compat": "~6.4.0",
    "smart-eval-kinvey": "git+https://github.com/autochair/SmartEvalKinvey.git#master",
    "tns-core-modules": "~5.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngtools/webpack": "~7.3.3",
    "prettier": "^1.16.4",
    "husky": "^1.3.1",
    "prompt": "^1.0.0",
    "lint-staged": "^8.1.4",
    "nativescript-dev-sass": "~1.6.1",
    "nativescript-dev-webpack": "~0.20.2",
    "nativescript-dev-typescript": "~0.8.0",
    "terser-webpack-plugin": "~1.2.3",
    "tns-platform-declarations": "~5.2.1",
    "tslint": "~5.13.0",
    "typescript": "~3.1.6"
  },
  "lint-staged": {
    "*.{ts, js, json, css, scss, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  }
}
