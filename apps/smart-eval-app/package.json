{
  "name": "maxmobility",
  "description": "NativeScript Application",
  "license": "SEE LICENSE IN <your-license-filename>",
  "readme": "NativeScript Application",
  "repository": "<fill-your-repository-here>",
  "nativescript": {
    "id": "com.permobil.smarteval",
    "tns-android": {
      "version": "5.2.1"
    }
  },
  "scripts": {
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "nuke": "rm -rf node_modules platforms hooks",
    "lint": "tslint \"app/**/*.ts\"",
    "reset": "rm -rf hooks node_modules platforms && npm run css.clean && npm run js.clean",
    "clean": "npm run js.clean && npm run css",
    "css.clean": "find app -type f -iname '*.css' -and ! -iname 'font-awesome.min.css' -delete",
    "css.generate": "node -e 'require(\"./node_modules/nativescript-dev-sass/lib/converter.js\").convert(console, \".\");'",
    "css": "npm run css.clean && npm run css.generate",
    "js.clean": "npx rimraf -- app/**/*.js",
    "generate-android-snapshot": "generate-android-snapshot --targetArchs arm,arm64,ia32 --install",
    "start-android-bundle": "npm run ns-bundle --android --run-app",
    "start-ios-bundle": "npm run ns-bundle --ios --run-app",
    "build-geolocation": "cd ./plugins/nativescript-geolocation && npm i && tsc && rm -rf *.tgz && npm pack",
    "make-changelog": "github_changelog_generator -u PushTracker -p EvalApp",
    "postinstall": "node ./tools/xplat-postinstall.js"
  },
